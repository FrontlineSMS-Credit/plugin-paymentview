<?xml version="1.0" encoding="UTF-8"?>
<panel name="pnQuickDialCodes" columns="1" weightx="1" weighty="1" gap="9" top="9" right="9" bottom="9" left="9">
    <table name="tblQuickDialCodes" weightx="1" weighty="1" action="selectQuickDialCode(this)" perform="showQuickDialCode">
        <header>
            <column text="i18n.plugins.paymentview.settings.ussd.code" width="120"/>
            <column text="i18n.plugins.paymentview.common.network.operator" width="120" />
            <column text="i18n.plugins.paymentview.common.description" />
        </header>
    </table>
    <panel gap="5" weightx="1">
        <button name="btnAddQuickDialCode" text="i18n.action.add" action="showNewQuickDialCodeDialog"/>
        <button name="btnEditQuickDialCode" text="i18n.action.edit" enabled="false" />
        <button name="btnDeleteQuickDialCode" text="i18n.action.delete" enabled="false"/>
    </panel>
</panel>