<?xml version="1.0" encoding="ISO-8859-1"?>
<dialog bottom="9" 
	columns="1" gap="15" left="9" name="clientDetailsDialog"
	right="9" text="Payment Service(s)"
	top="9" resizable="true" modal="true" closable="true" close="removeDialog(this)" >
	
	<label text="Select a Payment Service" />
	<combobox editable ="false" colspan="2" text="Select" name="cmbPaymentServices" columns="50"/>
	<panel colspan="3" weightx="1">
		<button icon="/icons/cross.png" text="Cancel"
			action="removeDialog" />
		<panel weightx="1" />
		<button icon="/icons/right.png" text="Next"
			action="authorize" />
	</panel>
</dialog>